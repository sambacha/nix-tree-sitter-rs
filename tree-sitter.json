{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "nix",
      "camelcase": "Nix",
      "title": "Nix ",
      "scope": "source.nix",
      "file-types": [
        "nix"
      ],
      "injection-regex": "^nix$",
      "class-name": "TreeSitterNix"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Nix/NixOS",
    "authors": [
      {
        "name": "sam bacha"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-nix"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}